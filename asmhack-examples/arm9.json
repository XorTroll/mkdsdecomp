{
    "$arm_flags": "-march=armv5te -mtune=arm946e-s -mthumb-interwork $${arm_flags}",
    "$c_flags": "${arm_flags} $${c_flags}",
    "$cpp_flags": "${c_flags} $${cpp_flags}",
    "$asm_flags": "${arm_flags} $${asm_flags}",
    "$ld_flags": "$${ld_flags}",
    
    "c_flags": "${c_flags}",
    "cpp_flags": "${cpp_flags}",
    "asm_flags": "${asm_flags}",
    "ld_flags": "${ld_flags}",
    
    "includes": [
        ["../re-export/include", false],
        ["include", false]
    ],
    "regions": [
        {
            "dest": "main",
            "compress": true,
            "sources": [
                ["source", true],
                ["asmhack-source", true],
                ["../re-export/source", true]
            ]
        }
    ],
    
    "arenaLo": "0x0200F01C",
    "symbols": "../re-export/mkds-eu-symbols.x"
}
